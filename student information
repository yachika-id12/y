#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
struct student
{
    int marks;
    char name[25];
    int age;
    int members;
};
int main()
{
    struct student s[100];
 int n,i;
 scanf("%d",&n);
 for(i=1;i<=n;i++){
  scanf("%s",s[i].name);
  scanf("%d",&s[i].age);
 scanf("%d",&s[i].marks);
  
}
   for(i=1;i<=n;i++)
{
    printf("Name: %s\n",s[i].name);
    printf("Age: %d\n",s[i].age);
    printf("Total Marks: %d\n",s[i].marks);
    printf("\n");
}
}
